<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mc.app.repository.TaleRepository">

    <select id="getByTaleId" resultType="Tale">
        SELECT *
        FROM page
        WHERE tale_id = #{taleId}
    </select>

</mapper>
